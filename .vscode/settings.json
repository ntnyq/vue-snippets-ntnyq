{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.configPath": "./prettier.config.mjs",
  "eslint.validate": [
    "vue",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "json.schemas": [
    {
      "fileMatch": ["/snippets/*.json"],
      "url": "/.vscode/schemas/snippets.schema.json"
    }
  ],
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "Cves",
    "NPMRC",
    "ofetch",
    "pinia",
    "tsup"
  ]
}
