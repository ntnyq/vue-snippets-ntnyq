{
  "publisher": "ntnyq",
  "name": "vue-snippets-ntnyq",
  "displayName": "Vue Snippets Ntnyq",
  "preview": true,
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "Opinionable VSCode extension for VueJS snippets.",
  "keywords": [
    "snippets",
    "vue",
    "vue-snippets"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vue-snippets-ntnyq",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/vue-snippets-ntnyq.git"
  },
  "bugs": {
    "url": "https://github.com/ntnyq/vue-snippets-ntnyq/issues/new"
  },
  "icon": "./res/icon.png",
  "files": [
    "LICENSE",
    "dist/*",
    "res/*",
    "schemas/*",
    "snippets/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint",
    "release:publish": "vsce publish --no-dependencies",
    "release:version": "bumpp"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.8",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/sfc.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/router.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/pinia.json"
      }
    ]
  },
  "categories": [
    "Snippets"
  ],
  "extensionDependencies": [
    "Vue.volar"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
