{
  "publisher": "ntnyq",
  "name": "vue-snippets-ntnyq",
  "displayName": "Vue Snippets Ntnyq",
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "preview": true,
  "description": "Opinionable VSCode extension for VueJS snippets.",
  "keywords": [
    "vue",
    "snippets",
    "vue-snippets"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vue-snippets-ntnyq",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/vue-snippets-ntnyq.git"
  },
  "icon": "./res/icon.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^2.8.2",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^20.16.3",
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^2.32.0",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "extensionDependencies": [
    "Vue.volar"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/sfc.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/router.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/pinia.json"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/ntnyq/vue-snippets-ntnyq/issues/new"
  },
  "categories": [
    "Snippets"
  ],
  "engines": {
    "vscode": "^1.92.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
